{
  "name": "basicOntimize",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve --port 4299",
    "start-local": "npm run build-themes && ng serve --configuration=local --port 4299",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compose": "npm run build-themes && ng build --configuration=compose --baseHref=/app/",
    "local": "npm run build-themes && ng build --configuration=local --baseHref=/app/",
    "production": "npm run build-themes && ng build --configuration=production --baseHref=/app/",
    "staging": "npm run build-themes && ng build --configuration=staging --baseHref=/app/",
    "build-themes": "node-sass ./src/assets/themes/ -o ./src/assets/themes/ --recursive"
  },
  "private": true,
  "engines": {
    "node": "^14"
  },
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^14.2.7",
    "@angular/material-moment-adapter": "^14.2.7",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/platform-server": "^15.2.10",
    "@angular/pwa": "^0.803.29",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@angular/upgrade": "^15.2.10",
    "core-js": "^3.6.4",
    "ontimize-web-ngx": "15.0.0-beta.0",
    "ngx-material-timepicker": "5.4.3",
    "node-sass": "4.14.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~3.6.0",
    "@types/node": "~8.9.4",
    "acorn": "5.7.4",
    "handlebars": "4.7.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "kind-of": "6.0.3",
    "minimist": "1.2.5",
    "moment": "^2.22.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.9.5"
  }
}
