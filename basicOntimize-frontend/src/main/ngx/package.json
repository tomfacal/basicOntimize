{
  "name": "basicOntimize",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve --port 4299",
    "start-local": "npm run build-themes && ng serve --configuration=local --port 4299",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compose": "npm run build-themes && ng build --configuration=compose --baseHref=/app/",
    "local": "npm run build-themes && ng build --configuration=local --baseHref=/app/",
    "production": "npm run build-themes && ng build --configuration=production --baseHref=/app/",
    "staging": "npm run build-themes && ng build --configuration=staging --baseHref=/app/",
    "build-themes": "node-sass ./src/assets/themes/ -o ./src/assets/themes/ --recursive"
  },
  "private": true,
  "engines": {
    "node": "^12"
  },
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/flex-layout": "~9.0.0-beta.31",
    "@angular/forms": "~9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "^9.2.4",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/platform-server": "^9.1.13",
    "@angular/pwa": "^0.803.29",
    "@angular/router": "~9.1.13",
    "@angular/service-worker": "~9.1.13",
    "@angular/upgrade": "^9.1.13",
    "core-js": "^3.6.4",
    "ngx-material-timepicker": "5.4.3",
    "node-sass": "4.14.1",
    "rxjs": "~6.6.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "~9.1.15",
    "@angular/compiler-cli": "~9.1.13",
    "@types/jasmine": "~3.3.8",
    "@types/node": "~8.9.4",
    "acorn": "5.7.4",
    "handlebars": "4.7.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "kind-of": "6.0.3",
    "minimist": "1.2.5",
    "moment": "^2.22.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  }
}
